{
    "name": "toggle",
    "displayName": "Toggle",
    "description": "Toggle settings in Visual Studio Code",
    "version": "0.0.2",
    "publisher": "rebornix",
    "engines": {
        "vscode": "^1.55.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:toggle"
    ],
    "icon": "round-toggle.png",
    "main": "./out/src/extension",
    "capabilities": {
        "untrustedWorkspaces": {
            "supported": true
        }
    },
    "contributes": {
        "commands": [
            {
                "command": "toggle",
                "title": "Toggle"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.0",
        "@types/node": "^12.12.0",
        "@types/vscode": "^1.55.0",
        "mocha": "^10.1.0",
        "typescript": "^4.2.2"
    }
}
