{
    "name": "toggle",
    "displayName": "Toggle",
    "description": "Toggle settings in Visual Studio Code",
    "version": "0.0.1",
    "publisher": "rebornix",
    "engines": {
        "vscode": "^1.9.0"
    },
    "bugs": {
        "url": "https://github.com/rebornix/vscode-toggle/issues"
    },
    "homepage": "https://github.com/rebornix/vscode-toggle",
    "repository": {
        "type": "git",
        "url": "https://github.com/rebornix/vscode-toggle"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:toggle"
    ],
    "icon": "round-toggle.png",
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "toggle",
            "title": "Toggle"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
